	if( i > 1 ) {		sSQLs="SELECT Correct_Typ, Correct_AMT, DivCorr_AMT, Pay_Date, Subcon_AMT, Subcon_Cnt, \n";		sSQLs+="REPLACE(REPLACE(Survey_Const, CHR(10), ' '), CHR(13), ' ') as Survey_Const \n";		sSQLs+="FROM HADO_VT_Survey_CTX \n";		sSQLs+="WHERE RTRIM(Mng_No)=? \n";		sSQLs+="	AND Current_Year=? \n";		sSQLs+="	AND Oent_GB=? \n";
