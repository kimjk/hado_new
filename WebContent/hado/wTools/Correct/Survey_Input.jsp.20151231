		/* 2014년 제조/용역 부당한 하도급대금 결정 복합판단 보정을 위해 결과값 미리 가져오기 끝 */
		// 입력되어 있는 값이 없을때.....		if( ogb.equals("1") || ogb.equals("3") || (currentYear>=2012 && ogb.equals("2")) ) {			// 원사업자 쿼리			sSQLs="SELECT oent.*, soent.*, oent.Identity_Q_CD icd, oent.Identity_Q_GB igb, \n";			sSQLs+="oent.suv_total+NVL(soent.suv_total,0) survey_total FROM ( \n";			sSQLs+="	SELECT Mng_No, Current_Year, Oent_GB, Oent_Q_CDNM, Money_GB oent_money_gb,  \n"; 			sSQLs+="	Rank, MAX(SOent_Q_CD) SOent_Q_CD, MAX(SOent_Q_GB) SOent_Q_GB, \n"; 			sSQLs+="	Identity_Q_CD, Identity_Q_GB, SUM(suv_total) suv_total FROM ( \n";			sSQLs+="		SELECT Mng_No,Current_Year,Oent_GB,Oent_Q_CD,Oent_Q_GB,Oent_Q_CDNM,Money_GB,Rank, \n";			sSQLs+="		NVL(suv_a+suv_b+suv_c+suv_d+suv_e+suv_f+suv_g+suv_h+suv_i+suv_j+suv_k+suv_l+suv_m+suv_n,0) suv_total, \n";			sSQLs+="		SOent_Q_CD,SOent_Q_GB,Identity_Q_CD,Identity_Q_GB FROM ( \n";			sSQLs+="			SELECT a.Mng_No, a.Current_Year, a.Oent_GB, b.Identity_Q_CD, b.Identity_Q_GB, \n";			sSQLs+="			a.Oent_Q_CD, a.Oent_Q_GB, b.Oent_Q_CDNM, b.Money_GB, b.Rank, b.SOent_Q_CD, b.SOent_Q_GB, \n";			sSQLs+="			CASE a.a WHEN '1' THEN (CASE chk_v_a WHEN '1' THEN 1 ELSE 0 END) ELSE 0 END suv_a, \n";			sSQLs+="			CASE a.b WHEN '1' THEN (CASE chk_v_b WHEN '1' THEN 1 ELSE 0 END) ELSE 0 END suv_b, \n"; 			sSQLs+="			CASE a.c WHEN '1' THEN (CASE chk_v_c WHEN '1' THEN 1 ELSE 0 END) ELSE 0 END suv_c, \n"; 			sSQLs+="			CASE a.d WHEN '1' THEN (CASE chk_v_d WHEN '1' THEN 1 ELSE 0 END) ELSE 0 END suv_d, \n"; 			sSQLs+="			CASE a.e WHEN '1' THEN (CASE chk_v_e WHEN '1' THEN 1 ELSE 0 END) ELSE 0 END suv_e, \n"; 			sSQLs+="			CASE a.f WHEN '1' THEN (CASE chk_v_f WHEN '1' THEN 1 ELSE 0 END) ELSE 0 END suv_f, \n"; 			sSQLs+="			CASE a.g WHEN '1' THEN (CASE chk_v_g WHEN '1' THEN 1 ELSE 0 END) ELSE 0 END suv_g, \n"; 			sSQLs+="			CASE a.h WHEN '1' THEN (CASE chk_v_h WHEN '1' THEN 1 ELSE 0 END) ELSE 0 END suv_h, \n"; 			sSQLs+="			CASE a.i WHEN '1' THEN (CASE chk_v_i WHEN '1' THEN 1 ELSE 0 END) ELSE 0 END suv_i, \n"; 			sSQLs+="			CASE a.j WHEN '1' THEN (CASE chk_v_j WHEN '1' THEN 1 ELSE 0 END) ELSE 0 END suv_j, \n"; 			sSQLs+="			CASE a.k WHEN '1' THEN (CASE chk_v_k WHEN '1' THEN 1 ELSE 0 END) ELSE 0 END suv_k, \n"; 			sSQLs+="			CASE a.l WHEN '1' THEN (CASE chk_v_l WHEN '1' THEN 1 ELSE 0 END) ELSE 0 END suv_l, \n"; 			sSQLs+="			CASE a.m WHEN '1' THEN (CASE chk_v_m WHEN '1' THEN 1 ELSE 0 END) ELSE 0 END suv_m, \n"; 			sSQLs+="			CASE a.n WHEN '1' THEN (CASE chk_v_n WHEN '1' THEN 1 ELSE 0 END) ELSE 0 END suv_n  FROM ( \n";			sSQLs+="				SELECT * FROM HADO_TB_Oent_Question WHERE Current_Year='"+currentYear+"' \n";			sSQLs+="			) b LEFT JOIN ( \n";			sSQLs+="				SELECT * FROM "+currentAnswer+" WHERE Mng_No='"+omngno+"' AND Current_Year='"+currentYear+"' \n";			sSQLs+="			) a ON a.Oent_Q_CD=b.Oent_Q_CD AND a.Oent_Q_GB=b.Oent_Q_GB AND a.Oent_GB=b.Oent_GB \n";			sSQLs+="			WHERE b.Oent_GB='"+ogb+"' AND (b.Money_GB='1' OR b.Money_GB='2') \n";			sSQLs+="		) suv \n";			sSQLs+="	) suv_oent \n";			sSQLs+="	GROUP BY Mng_No, Current_Year, Oent_GB, Oent_Q_CDNM, Money_GB, Rank, Identity_Q_CD, Identity_Q_GB \n";			sSQLs+=") oent LEFT JOIN (\n";			//수급사업자 쿼리			sSQLs+="	SELECT Mng_No, Current_Year, Oent_GB,SOent_Q_CD, SOent_Q_CDNM, Money_GB, Identity_Q_CD, Identity_Q_GB, \n";			sSQLs+="	SUM(suv_total) suv_total FROM ( \n";			sSQLs+="		SELECT Mng_No, Current_Year, Oent_GB, SOent_Q_CD, SOent_Q_GB, SOent_Q_CDNM, Money_GB,Rank, Identity_Q_CD, \n";			sSQLs+="		NVL(suv_a+suv_b+suv_c+suv_d+suv_e+suv_f+suv_g+suv_h+suv_i+suv_j+suv_k+suv_l+suv_m+suv_n,0) suv_total, \n";			sSQLs+="		Identity_Q_GB FROM ( \n";
			if( currentYear >= 2012 ) {				sSQLs+="			SELECT SUBSTR(a.Mng_No,0,8) Mng_No,a.Current_Year,b.Oent_GB,b.SOent_Q_CD,b.SOent_Q_GB, \n";			} else if( currentYear == 2011 ) {				sSQLs+="			SELECT SUBSTR(a.Mng_No,0,9) Mng_No,a.Current_Year,b.Oent_GB,b.SOent_Q_CD,b.SOent_Q_GB, \n";			} else {				sSQLs+="			SELECT SUBSTR(a.Mng_No,0,10) Mng_No,a.Current_Year,b.Oent_GB,b.SOent_Q_CD,b.SOent_Q_GB, \n";			}
			sSQLs+="			b.SOent_Q_CDNM,b.Money_GB,b.Rank,b.Identity_Q_CD,b.Identity_Q_GB, \n"; 			sSQLs+="			CASE a.a WHEN '1' THEN (CASE chk_v_a WHEN '1' THEN 1 ELSE 0 END) ELSE 0 END suv_a, \n";			sSQLs+="			CASE a.b WHEN '1' THEN (CASE chk_v_b WHEN '1' THEN 1 ELSE 0 END) ELSE 0 END suv_b, \n"; 			sSQLs+="			CASE a.c WHEN '1' THEN (CASE chk_v_c WHEN '1' THEN 1 ELSE 0 END) ELSE 0 END suv_c, \n";			sSQLs+="			CASE a.d WHEN '1' THEN (CASE chk_v_d WHEN '1' THEN 1 ELSE 0 END) ELSE 0 END suv_d, \n";			sSQLs+="			CASE a.e WHEN '1' THEN (CASE chk_v_e WHEN '1' THEN 1 ELSE 0 END) ELSE 0 END suv_e, \n";			sSQLs+="			CASE a.f WHEN '1' THEN (CASE chk_v_f WHEN '1' THEN 1 ELSE 0 END) ELSE 0 END suv_f, \n";			sSQLs+="			CASE a.g WHEN '1' THEN (CASE chk_v_g WHEN '1' THEN 1 ELSE 0 END) ELSE 0 END suv_g, \n"; 			sSQLs+="			CASE a.h WHEN '1' THEN (CASE chk_v_h WHEN '1' THEN 1 ELSE 0 END) ELSE 0 END suv_h, \n"; 			sSQLs+="			CASE a.i WHEN '1' THEN (CASE chk_v_i WHEN '1' THEN 1 ELSE 0 END) ELSE 0 END suv_i, \n"; 			sSQLs+="			CASE a.j WHEN '1' THEN (CASE chk_v_j WHEN '1' THEN 1 ELSE 0 END) ELSE 0 END suv_j, \n"; 			sSQLs+="			CASE a.k WHEN '1' THEN (CASE chk_v_k WHEN '1' THEN 1 ELSE 0 END) ELSE 0 END suv_k, \n"; 			sSQLs+="			CASE a.l WHEN '1' THEN (CASE chk_v_l WHEN '1' THEN 1 ELSE 0 END) ELSE 0 END suv_l, \n"; 			sSQLs+="			CASE a.m WHEN '1' THEN (CASE chk_v_m WHEN '1' THEN 1 ELSE 0 END) ELSE 0 END suv_m, \n";			sSQLs+="			CASE a.n WHEN '1' THEN (CASE chk_v_n WHEN '1' THEN 1 ELSE 0 END) ELSE 0 END suv_n  FROM ( \n";			sSQLs+="				SELECT * FROM HADO_TB_SOent_Question WHERE Current_Year='"+currentYear+"' \n";			sSQLs+="			) b LEFT JOIN ( \n";			sSQLs+="				SELECT * FROM (\n";			sSQLs+="					SELECT C1.*,C2.Sent_Status,C2.Addr_Status \n";			sSQLs+="					FROM HADO_TB_SOent_Answer_"+currentYear+" C1 \n";			sSQLs+="					LEFT JOIN HADO_TB_Subcon_"+currentYear+" C2 \n";			sSQLs+="					ON C1.Mng_No=C2.Child_Mng_No \n";			sSQLs+="						AND C1.Current_Year=C2.Current_Year \n";			sSQLs+="						AND C1.Sent_No=C2.Sent_No \n";			sSQLs+="						AND C1.Oent_GB=C2.Oent_GB \n";			sSQLs+="				) \n";			if( currentYear >= 2012 ) {				sSQLs+="				WHERE SUBSTR(Mng_No,0,8)='"+omngno+"' AND Current_Year='"+currentYear+"' \n";			} else if( currentYear == 2011 ) {				sSQLs+="				WHERE SUBSTR(Mng_No,0,9)='"+omngno+"' AND Current_Year='"+currentYear+"' \n";			} else if( currentYear >= 2007 ) {				sSQLs+="				WHERE SUBSTR(Mng_No,0,10)='"+omngno+"' AND Current_Year='"+currentYear+"' \n";			} else if( currentYear >= 2006 ) {				sSQLs+="				WHERE SUBSTR(RTrim(Mng_No),0,9)='"+omngno+"' AND Current_Year='"+currentYear+"' \n";			} else {				sSQLs+="				WHERE SUBSTR(RTrim(Mng_No),0,7)='"+omngno+"' AND Current_Year='"+currentYear+"' \n";			}			sSQLs+="				AND Sent_Status='1' \n";			sSQLs+="			) a ON a.SOent_Q_CD=b.SOent_Q_CD AND a.SOent_Q_GB=b.SOent_Q_GB AND a.Oent_GB=b.Oent_GB \n";			sSQLs+="			WHERE b.Oent_GB='"+ogb+"' \n"; 			sSQLs+="		) suv \n";			sSQLs+="	) suv_soent  \n";			sSQLs+="	GROUP BY Mng_No, Current_Year, Oent_GB, SOent_Q_CD, SOent_Q_CDNM, Money_GB, Identity_Q_CD, Identity_Q_GB \n";			sSQLs+=") soent ON oent.Identity_Q_CD=soent.Identity_Q_CD AND oent.Identity_Q_GB=soent.Identity_Q_GB \n";			sSQLs+="ORDER BY oent.Oent_Money_GB, Oent.Rank \n";		} else if( currentYear<2012 && ogb.equals("2") ) {			sSQLs="SELECT * FROM ( \n";			sSQLs+="	SELECT * FROM HADO_TB_Survey_Text WHERE Oent_GB='"+ogb+"' AND Current_Year='"+currentYear+"' \n";			sSQLs+=") a LEFT JOIN ( \n";			sSQLs+="	SELECT * FROM HADO_TB_Survey_Print2 WHERE Mng_No='"+omngno+"' AND Oent_GB='"+ogb+"' AND Current_Year='"+ currentYear+"' \n";			sSQLs+=") b ON a.Current_Year=b.Current_Year AND a.Oent_GB=b.Oent_GB \n";			sSQLs+="ORDER BY a.Money_GB, a.Rank \n";		}
