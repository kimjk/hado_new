		//System.out.print(sSQLs+"\n\n");
		pstmt = conn.prepareStatement(sSQLs);		pstmt.setString(1,ogb);		pstmt.setString(2,currentYear+"");		pstmt.setString(3,omngno);		pstmt.setString(4,currentYear+"");		pstmt.setString(5,ogb);		rs = pstmt.executeQuery();
