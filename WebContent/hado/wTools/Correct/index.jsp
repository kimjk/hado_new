y;					if(i!=0)	{						pmoney=","+pmoney;					}				} else {					pmoney=money.substr(i,1)+pmoney;				}			}			return pmoney;		}		function clearstring(s) {			var pstr, sstr, iz;			sstr=s;			pstr="";			for(iz=0;iz<sstr.length;iz++) {				if(!isNaN(sstr.substr(iz,1))||sstr.substr(iz,1)==".") {					pstr=pstr+sstr.substr(iz,1);				}			}			return pstr;		}		function conf(gesu) {			var msg="";			var obj=null;			var obj1=null;			var obj2=null;			var obj3=null;			for(i=1;i<gesu;i++) {				obj=document.getElementById("smoney"+i);				if(obj.value!="0") {					eval("obj2=obj.form.money"+i+"[0]");					if(obj2.checked==false) {						msg+="법위반이거나 법위반인정이 아닌경우 금액을 넣으면 안됩니다.";						break;					}				}			}			if(obj!=null && msg=="") {				obj1=document.getElementById("sctx").form.sctx;				obj2=document.getElementById("h_sctx");				for(i=0; i<12; i++) {					if(obj1[i].checked==true) {						obj2.value=obj1[i].value;						break;					}				}				obj1=document.getElementById("sconst");				obj2=document.getElementById("h_sconst");				obj2.value=obj1.value;				obj1=document.getElementById("ctxmoney1");				obj2=document.getElementById("h_ctxmoney1");				obj2.value=obj1.value;				obj1=document.getElementById("ctxmoney2");				obj2=document.getElementById("h_ctxmoney2");				obj2.value=obj1.value;				obj1=document.getElementById("ctxdate1");				obj2=document.getElementById("h_ctxdate1");				obj2.value=obj1.value;				obj1=document.getElementById("ctxdate2");				obj2=document.getElementById("h_ctxdate2");				obj2.value=obj1.value;				obj1=document.getElementById("ctxdate3");				obj2=document.getElementById("h_ctxdate3");				obj2.value=obj1.value;				obj1=document.getElementById("submoney");				obj2=document.getElementById("h_submoney");				obj2.value=obj1.value;				obj1=document.getElementById("ctxsu2");				obj2=document.getElementById("h_ctxsu2");				obj2.value=obj1.value;				for(i=1; i<12; i++) {					obj1=document.getElementById("