lection Processing =====================================*/	int currentYear = st_Current_Year_n;	StringBuffer sbSQLs1 = new StringBuffer();	sbSQLs1.append("SELECT COMMON_CD, COMMON_NM \n");	sbSQLs1.append("FROM COMMON_CD \n");	sbSQLs1.append("WHERE ADDON_GB=? ");	if(currentYear>=2012) {		sbSQLs1.append("AND COMMON_GB='010'");	}	try {		resource = new ConnectionResource();		conn = resource.getConnection();		// 제조 업종 배열 생성		pstmt = conn.prepareStatement(sbSQLs1.toString());		pstmt.setString(1,"1");		rs = pstmt.executeQuery();		while (rs.next()) {			arrDCode.add( rs.getString("COMMON_CD")==null ? "":rs.getString("COMMON_CD").trim() );			arrDName.add( rs.getString("COMMON_NM")==null ? "":rs.getString("COMMON_NM").trim() );		}		rs.close();	} catch(Exception e) {		e.printStackTrace();	} finally {		if ( rs != null ) try{rs.close();}catch(Exception e){}		if ( pstmt != null ) try{pstmt.close();}catch(Exception e){}		if ( conn != null ) try{conn.close();}catch(Exception e){}		if ( resource != null ) resource.release();	}	try {		resource = new ConnectionResource();		conn = resource.getConnection();		// 건설 업종 배열 생성		pstmt = conn.prepareStatement(sbSQLs1.toString());		pstmt.setString(1,"2");		rs = pstmt.executeQuery();		while (rs.next()) {			arrCCode.add( rs.getString("COMMON_CD")==null ? "":rs.getString("COMMON_CD").trim() );			arrCName.add( rs.getString("COMMON_NM")==null ? "":rs.getString("COMMON_NM").trim() );		}		rs.close();	} catch(Exception e) {		e.printStackTrace();	} finally {		if ( rs != null ) try{rs.close();}catch(Exception e){}		if ( pstmt != null ) try{pstmt.close();}catch(Exception e){}		if ( conn != null ) try{conn.close();}catch(Exception e){}		if ( resource != null ) resource.release();	}	try {		resource = new ConnectionResource();		conn = resource.getConnection();		// 용역 업종 배열 생성		pstmt = conn.prepareStatement(sbSQLs1.toString());		pstmt.setString(1,"3");		rs = pstmt.executeQuery();		while (rs.next()) {			arrSrvCode.add( rs.getString("COMMON_CD")==null ? "":rs.getString("COMMON_CD").trim() );			arrSrvName.add( rs.getString("COMMON_NM")==null ? "":rs.getString("COMMON_NM").trim() );		}		rs.close();	} catch(Exception e) {		e.printStackTrace();	} finally {		if ( rs != null ) try{rs.close();}catch(Exception e){}		if ( pstmt != null ) try{pstmt.close();}catch(Exception e){}		if ( conn != null ) try{conn.close();}catch(Exception e){}		if ( resource != null ) resource.release();	}		// 2010년 담당자 세팅 방법 변경 (HADO_TB_DEPT_HISTORY)		StringBuffer sbSQLs2 = new StringBuffer();		if(st_Current_Year_n >= 2019) {			sbSQLs2.append("SELECT CVSNO DEPT_SEQ, Center_Name, User_Name, DEPT_NAME \n");			sbSQLs2.append("FROM HADO_TB_DEPT_HISTORY_" + st_Current_Year_n + " \n");			sbSQLs2.append("WHERE Current_Year=? \n");			sbSQLs2.append("GROUP BY CVSNO, Center_Name, User_Name, DEPT_NAME \n");			sbSQLs2.append("ORDER BY Center_Name, User_Name, DEPT_NAME");		} else if( st_Current_Year_n>= 2014) {			sbSQLs2.append("SELECT CVSNO DEPT_SEQ, Center_Name,User_Name \n");			sbSQLs2.append("FROM HADO_TB_DEPT_HISTORY_" + st_Current_Year_n + " \n");			sbSQLs2.append("WHERE Current_Year=? \n");			sbSQLs2.append("GROUP BY CVSNO, Center_Name, User_Name \n");			sbSQLs2.append("ORDER BY Center_Name, User_Name");		} else if( st_Current_Year_n>= 2010) {			sbSQLs2.append("SELECT CVSNO DEPT_SEQ, Center_Name,User_Name \n");			sbSQLs2.append("FROM HADO_TB_DEPT_HISTORY \n");			sbSQLs2.append("WHERE Current_Year=? \n");			sbSQLs2.append("GROUP BY CVSNO, Center_Name, User_Name \n");			sbSQLs2.append("ORDER BY Center_Name, User_Name");		} else {			// 담당자별 배열 생성			sbSQLs2.append("SELECT DEPT_SEQ, CENTER_NAME, USER_NAME FROM HADO_TB_FTC_USER \n");			sbSQLs2.append("ORDER BY CENTER_NAME, USER_NAME");		}	try {		resource = new ConnectionResource();		conn = resource.getConnection();		pstmt = conn.prepareStatement(sbSQLs2.toString());		if( st_Current_Year_n>= 2010) {			pstmt.setString(1,st_Current_Year);		}		rs = pstmt.executeQuery();		while(rs.next()) {			arrDeptSeq.add( rs.getString("DEPT_SEQ")==null ? "":rs.getString("DEPT_SEQ").trim() );			arrDeptCode.add( selDeptCode(rs.getString("CENTER_NAME")==null ? "":rs.getString("CENTER_NAME").trim()) );			arrDamName.add( rs.getString("USER_NAME")==null ? "":rs.getString("USER_NAME").trim() );						if(st_Current_Year_n >= 2019) {				arrDeptCode2.add( selDeptCode2(rs.getString("DEPT_NAME")==null ? "":rs.getString("DEPT_NAME").trim()) );			}		}		rs.close();	} catch(Exception e) {		e.printStackTrace();	} finally {		if(rs!=null)		try{rs.close();}	catch(Exception e){}		if(pstmt!=null)		try{pstmt.close();}	catch(Exception e){}		if(conn!=null)		try{conn.close();}	catch(Exception e){}		if(resource!=null)	resource.release();	}/*=====================================================================================================*/%><%!	public String selDeptCode(String str) {		String sReturnStr = "";		if(str.equals("서울사무소") || str.equals("서울 사무소") ) {			sReturnStr = "S";		} else if(str.equals("대구사무소") || str.equals("대구 사무소")) {			sReturnStr = "K";		} else if(str.equals("대전사무소") || str.equals("대전 사무소")) {			sReturnStr = "J";		} else if(str.equals("광주사무소") || str.equals("광주 사무소")) {			sReturnStr = "G";		} else if(str.equals("부산사무소") || str.equals("부산 사무소") ) {			sReturnStr = "B";		} else {			sReturnStr = "H";		}		return sReturnStr ;	}public String selDeptCode2(String str){	String sReturnStr = "";	String subStr = str.substring(0, 2);	int lng = str.length();	if(subStr.equals("기업") && lng > 6)sReturnStr = "H1";	else if(subStr.equals("제조") && lng > 6) sReturnStr = "H2";	else if(subStr.equals("건설") && lng > 6) sReturnStr = "H3";	else if(subStr.equals("건설") && lng == 6) sReturnStr = "S1";	else if(subStr.equals("제조") && lng == 6) sReturnStr = "S2";	else sReturnStr = "";	return sReturnStr ;}%><html><head>	<title>【관리】하도급거래 서면실태조사</title>	<link rel="s